{
	"compilerOptions": {
		"moduleResolution": "node",
		"target": "ESNext",
		"module": "ESNext",
		"sourceMap": true,
		"incremental": true,
		"declaration": true,
		"alwaysStrict": true,
		"esModuleInterop": true,
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"forceConsistentCasingInFileNames": true
	}
}
